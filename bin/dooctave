#!/usr/bin/env bash

# GNU Octave / Xeus
# https://github.com/jupyter-xeus/xeus-octave/
if command -v octave >/dev/null 2>&1; then
  echo "⚙️ Installing Xeus Octave Jupyter kernel..."
  conda install -c conda-forge xeus-octave
else
    echo "⚙️ Octave is not installed. Please install Octave first."
fi